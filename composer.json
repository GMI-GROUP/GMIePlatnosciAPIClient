{
  "name": "gmigroup/gmi-client-pep",
  "description": "Client to API Polskie ePłatności",
  "license": "Apache-2.0",
  "type": "library",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "symfony/dotenv": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "Gmigroup\\Clients\\PeP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gmigroup\\Clients\\PeP\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0"
    }
  }
}
